<h1>Lista de Productos en Tiempo Real</h1>
<ul id="productos-lista">
    {{#each products}}
        <li>
            {{this.title}} - ${{this.price}}
            <button class="eliminar" data-id="{{this.id}}">‚ùå</button>
        </li>
